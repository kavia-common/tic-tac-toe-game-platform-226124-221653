# Frontend environment variables (example)
# The app will use REACT_APP_API_BASE if set, otherwise REACT_APP_BACKEND_URL, else default to http://localhost:3001

REACT_APP_API_BASE=http://localhost:3001
# or
# REACT_APP_BACKEND_URL=http://localhost:3001

# Optional environment values supported by the app (no need to change for local dev)
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS={}
REACT_APP_EXPERIMENTS_ENABLED=false
